<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" prefHeight="211.0" prefWidth="279.0" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.localization_process.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <Button fx:id="button1" mnemonicParsing="false" onAction="#setEN" prefHeight="26.0" prefWidth="300.0" text="%button1.text" />
    <Button fx:id="button2" mnemonicParsing="false" onAction="#setIR" prefHeight="26.0" prefWidth="300.0" text="%button2.text" />
    <Button fx:id="button3" mnemonicParsing="false" onAction="#setJP" prefHeight="26.0" prefWidth="300.0" text="%button3.text" />
    <Label fx:id="label1" text="%label1.text" />
</VBox>
